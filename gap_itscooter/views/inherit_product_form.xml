<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_inherit_itscooter" model="ir.ui.view">
        <field name="name">product.template.form.inherit.itscooter</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='barcode']" position="after">
                <field name='Qty'/>
            </xpath>
            <xpath expr="//field[@name='barcode']" position="replace">
                <field name='EAN' string='EAN'/>
            </xpath>
            <xpath expr="//field[@name='standar_price']" position="replace">
                <field name='Cost' string='Precio'/>
            </xpath>
            <xpath expr="//field[@name='default_code']" position="replace">
                <field name='SKU' string='SKU'/>
            </xpath>
        </field>
    </record>
</odoo>
