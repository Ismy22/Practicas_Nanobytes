<?xml version="1.0" encoding="utf-8"?>
<odoo>  
    <record id="res_partner_wizard_view_form" model="ir.ui.view">
        <field name="name">res.partner.wizard.form</field>
        <field name="model">res.partner.wizard</field>
        <field name="arch" type="xml">
            <form string="Test">
                <group>           
                    <field name="User"/>            
                </group>         
                <group>           
                    <field name="Project"/>            
                </group>         
            </form> 
        </field>
    </record>

    <record id="res_partner_wizard_action" model="ir.actions.act_window">   
        <field name="name">Model Wizard</field>   
        <field name="type">ir.actions.act_window</field>     
        <field name="res_model">res.partner.wizard</field>  
        <field name="view_mode">form</field>   
        <field name="view_id" ref="res.partner.wizard_view_form"/>   
        <field name="target">new</field>
    </record>

</odoo>