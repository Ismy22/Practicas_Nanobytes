<?xml version="1.0" encoding="utf-8"?>
<odoo>  

    <record id="view_res_user_wizard_form" model="ir.ui.view">
            <field name="name">res.user.wizard.form</field>
            <field name="model">res.user.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Select User and Project">
                    <group>
                        <field name="user"/>
                        <field name="project"/>
                    </group>
                    <footer>
                        <button string="Guardar" type="object" name="save_contact_wizard"/>
                        <button string="Cancelar" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>