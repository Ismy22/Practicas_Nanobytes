<odoo>
    <data>
        <record id="view_move_form_custom" model="ir.ui.view">
            <field name="name">account.move.form_inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_account_move_archivar" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@string='partner_id']" position="after">
                    <field name="archive" string="Archive" invisible='True'/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>