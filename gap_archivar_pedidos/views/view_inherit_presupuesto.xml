<odoo>
    <data>
        <record id="view_move_form_custom" model="ir.ui.view">
            <field name="name">account.move.form.custom</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header/button[@name='%(action_move_reconcile)s']" position="after">
                    <button name="action_archive" string="Archive" type="action" class="oe_highlight"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>